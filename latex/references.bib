}

@article{doi:10.1002/soej.12395,
	title = {The effects of differential income replacement and mortality on {U}.{S}. social security redistribution},
	volume = {0},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/soej.12395},
	doi = {10.1002/soej.12395},
	abstract = {We study redistribution via the U.S. Social Security retirement system for cohorts of men born during the second half of the 20th century. Our focus is on redistribution across race and education groups. The cohorts we study are younger than cohorts studied in previous, similar research and thus more exposed to recent increases in earnings inequality. All else equal, this should increase the degree of progressivity of Social Security redistribution due to the structure of the benefit formula. However, we find that redistribution is only modestly progressive for individuals born as late as 1980. Differential mortality rates across race and education groups are the primary explanation. While Black–White mortality gaps have narrowed some in recent years, they remain large and dull progressivity. Mortality gaps by education level are also large and unlike the gaps by race, they are widening, which puts additional regressive pressure on Social Security redistribution.},
	number = {0},
	journal = {Southern Economic Journal},
